<?xml version='1.0' encoding='utf-8'?>
<widget id="com.d4L.clo" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Check List Organiser</name>
    <description>
        Don't forget things anymore. Organize your activities in a convenient checklist and tick-off as you accomplish them...
    </description>
    <author email="support@developers4life.com" href="http://www.developers4life.com/">
        d4L
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
